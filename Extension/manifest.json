{
  "manifest_version": 3,
  "name": "CabCaddy",
  "description": "Base Level Extension",
  "permissions": ["activeTab", "storage", "tabs"],
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }
  ]
}